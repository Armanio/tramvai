{
  "extends": "../../../tsconfig.lib.json",
  "include": [
    "./src"
  ],
  "compilerOptions": {
    "module": "CommonJS",
    "declarationDir": "./lib",
    "outDir": "./lib",
    "rootDir": "./src",
    "strict": false,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictBindCallApply": false,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true
  }
}
